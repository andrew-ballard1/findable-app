{
	"expo": {
		"name": "findable-app",
		"slug": "findable-app",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon_60.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#3f72af"
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.tryfindable.www"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": [
			"sentry-expo"
		],
		"extra": {
			"eas": {
				"projectId": "aeaf7820-62bd-4665-ac94-4f8c8e084e54"
			}
		}
	}
}